{
    "defaultSeverity": "error",
    "extends": ["tslint:recommended"],
    "jsRules": {},
    "rules": {
      "max-line-length": [
        true,
        { "limit": 120, "ignore-pattern": "^import |^export {(.*?)}" }
      ],
      "object-literal-sort-keys": false,
      "trailing-comma": [
        true,
        {
          "singleline": "never",
          "esSpecCompliant": true
        }
      ]
    },
    "rulesDirectory": []
  }
  